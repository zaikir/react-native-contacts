{
  "name": "@kirz/react-native-contacts",
  "version": "0.0.5",
  "description": "React Native Contacts",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build && npx tsc-alias -p tsconfig.json && npx tsc-alias -p tsconfig.module.json && npx tsc-alias -p tsconfig.typescript.json",
    "release": "npm publish --access public"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/zaikir/react-native-contacts",
  "author": "Kirill Zaidulin <zaidulinkirill@gmail.com> (https://github.com/zaikir)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zaikir/react-native-contacts/issues"
  },
  "homepage": "https://github.com/zaikir/react-native-contacts#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "~18.2.0",
    "@types/react-native": "0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-universe": "^11.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-exports": "^1.0.0-beta.4",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.0.5",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-builder-bob": "^0.20.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "18.1.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
